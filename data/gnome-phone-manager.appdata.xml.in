<component type="desktop-application">
	<id>gnome-phone-manager.desktop</id>
	<metadata_license>CC0-1.0</metadata_license>
	<project_license>GPL-2.0+</project_license>
	<name>Phone Manager</name>
	<summary>Control your mobile phone from your desktop</summary>
	<description>
		<p>
			Phone Manager allows you to control your mobile phone. It uses the
			gnokii backend that typically works with older Nokia devices. Phone
			manager allows you to send SMS messages, view the address book on your
			phone, and receive notifications on the desktop when a new SMS arrives.
		</p>
	</description>
	<launchable type="desktop-id">gnome-phone-manager.desktop</launchable>
	<url type="homepage">https://live.gnome.org/PhoneManager/</url>
	<screenshots>
		<screenshot type="default">
			<image type="source">https://wiki.gnome.org/Attic/PhoneManager?action=AttachFile&amp;do=get&amp;target=send.png</image>
		</screenshot>
		<screenshot type="default">
			<image type="source">https://wiki.gnome.org/Attic/PhoneManager?action=AttachFile&amp;do=get&amp;target=prefs-2.png</image>
		</screenshot>
	</screenshots>
</component>
