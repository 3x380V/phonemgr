#: ../ui/gnome-phone-manager.schemas.in.h:1
msgid "Bluetooth address of the device to connect to"
msgstr "Bluetooth-adresse på enheten som skal kobles til"

#: ../ui/gnome-phone-manager.schemas.in.h:2
msgid "Bluetooth address of the device to connect to. Requires connection_type to be 1 to be used."
msgstr "Bluetooth-adresse på enheten som skal kobles til. Krever at connection_type er 1 for å brukes."

#: ../ui/gnome-phone-manager.schemas.in.h:3
msgid "The connection type used by gnome-phone-manager"
msgstr "Tilkoblingstypen som brukes av gnome-phone-manager"

#: ../ui/gnome-phone-manager.schemas.in.h:4
msgid "The connection type used by gnome-phone-manager: Bluetooth is 0 Serial 1 is 1 Serial 2 is 2 IrDa is 3 Other connection types are 4"
msgstr "Tilkoblingstypen som brukes av gnome-phone-manager: Bluetooth er 0, seriell 1 er 1, seriell 2 er 2, infrarød er 3, andre tilkoblingstyper er 4"

#: ../ui/gnome-phone-manager.schemas.in.h:5
msgid "The device node for the serial device to connect to"
msgstr "Enhetsnoden for seriell-enheten som skal kobles til"

#: ../ui/gnome-phone-manager.schemas.in.h:6
msgid "The device node for the serial device to connect to. Requires connection_type to be 4 to be used."
msgstr "Enhetsnoden for seriell-enheten som skal kobles til. Krever at connection_type er 4 for å brukes."

#: ../ui/gnome-phone-manager.schemas.in.h:7
msgid "Whether to play a sound alert when a new message comes in"
msgstr "Hvorvidt en lyd skal spilles av når en ny melding mottas"

#: ../ui/gnome-phone-manager.schemas.in.h:8
msgid "Whether to play a sound alert when a new message comes in."
msgstr "Hvorvidt en lyd skal spilles av når en ny melding mottas."

#: ../ui/gnome-phone-manager.schemas.in.h:9
msgid "Whether to popup new messages on the desktop"
msgstr "Hvorvidt et oppsprettsvindu med meldingen skal vises på skrivebordet"

#: ../ui/gnome-phone-manager.schemas.in.h:10
msgid "Whether to popup new messages on the desktop as soon as they're received, as opposed to showing them when clicking on the tray icon."
msgstr "Hvorvidt et oppsprettsvindu med meldingen skal vises på skrivebordet med en gang de mottas, i motsetning til å vise dem ved å klikke på programikonet."

#: ../ui/gnome-phone-manager.schemas.in.h:11
msgid "Whether to retry connecting to the mobile phone"
msgstr "Hvorvidt å forsøke å koble til mobiltelefonen på nytt"

#: ../ui/gnome-phone-manager.schemas.in.h:12
msgid "Whether to retry connecting to the mobile phone if the connection fails at some point."
msgstr "Hvorvidt å forsøke å koble til mobiltelefonen på nytt, hvis tilkoblingen feiler på et tidspunkt."

#: ../src/e-contact-entry.c:154
#, c-format
msgid "Cannot get contact: %s"
msgstr "Kan ikke hente kontakt: %s"

#: ../src/e-contact-entry.c:186
#, c-format
msgid "Could not find contact: %s"
msgstr "Kunne ikke finne kontakt: %s"

#: ../src/e-contact-entry.c:389
msgid "Cannot create searchable view."
msgstr "Kan ikke opprette søkbar visning."

#: ../src/e-contact-entry.c:844
msgid "Success"
msgstr "Suksess"

#: ../src/e-contact-entry.c:846
msgid "An argument was invalid."
msgstr "Ugyldig argument oppgitt."

#: ../src/e-contact-entry.c:848
msgid "The address book is busy."
msgstr "Adresseboken er opptatt."

#: ../src/e-contact-entry.c:850
msgid "The address book is offline."
msgstr "Adresseboken er frakoblet."

#: ../src/e-contact-entry.c:852
msgid "The address book does not exist."
msgstr "Adresseboken eksisterer ikke."

#: ../src/e-contact-entry.c:854
msgid "The \"Me\" contact does not exist."
msgstr "Kontakten «Meg» eksisterer ikke."

#: ../src/e-contact-entry.c:856
msgid "The address book is not loaded."
msgstr "Adresseboken er ikke lastet."

#: ../src/e-contact-entry.c:858
msgid "The address book is already loaded."
msgstr "Adresseboken er allerede lastet."

#: ../src/e-contact-entry.c:860
msgid "Permission was denied when accessing the address book."
msgstr "Nektet tilgang til adresseboken ."

#: ../src/e-contact-entry.c:862
msgid "The contact was not found."
msgstr "Kontakten ble ikke funnet."

#: ../src/e-contact-entry.c:864
msgid "This contact ID already exists."
msgstr "Denne kontakt-IDen finnes allerede."

#: ../src/e-contact-entry.c:866
msgid "The protocol is not supported."
msgstr "Protokollen er ikke støttet."

#: ../src/e-contact-entry.c:868
msgid "The operation was cancelled."
msgstr "Operavsjonen ble avbrutt."

#: ../src/e-contact-entry.c:870
msgid "The operation could not be cancelled."
msgstr "Handlingen kunne ikke avbrytes"

#: ../src/e-contact-entry.c:872
msgid "The address book authentication failed."
msgstr "Autentisering mot adresseboken feilet."

#: ../src/e-contact-entry.c:874
msgid "Authentication is required to access the address book and was not given."
msgstr "Autentisering mot adresseboken er nødvendig og ble ikke angitt."

#: ../src/e-contact-entry.c:876
msgid "A secure connection is not available."
msgstr "En sikker tilkobling er ikke tilgjengelig."

#: ../src/e-contact-entry.c:878
msgid "A CORBA error occurred whilst accessing the address book."
msgstr "En CORBA-feil oppsto under tilgang til adresseboken."

#: ../src/e-contact-entry.c:880
msgid "The address book source does not exist."
msgstr "Adressebokskilden finnes ikke"

#: ../src/e-contact-entry.c:882
#: ../src/e-contact-entry.c:885
msgid "An unknown error occurred."
msgstr "En ukjent feil oppsto."

