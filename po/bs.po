msgstr "Postavke Telefonskog Menadžera"

#: ../data/phonemgr.ui.h:16
msgid "Alerting"
msgstr "Obavijestiti"

#: ../data/phonemgr.ui.h:17
msgid "_Pop-up window for new messages"
msgstr "_Skočni prozor za nove poruke"

