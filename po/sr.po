msgid "Phone Manager"
msgstr "Управник телефона"

#: ../src/gnome-phone-manager.desktop.in.h:2
msgid "Receive and send text messages from your mobile phone"
msgstr "Примајте и шаљите текстуалне поруке са вашег мобилног телефона"

#: ../src/icon.c:59
msgid "Message arrived"
msgstr "Порука је пристигла"

#: ../src/icon.c:62
msgid "Connected"
msgstr "Повезани сте"

#: ../src/icon.c:66
msgid "Connecting to phone"
msgstr "Повезујем се са телефоном"

#: ../src/icon.c:70
msgid "Not connected"
msgstr "Нисте повезани"

#: ../src/main.c:38
msgid "Show model name of a specific device"
msgstr "Приказује назив модела посебног уређаја"

#: ../src/main.c:38 ../src/main.c:39
msgid "PORT"
msgstr "ВРАТА"

#: ../src/main.c:39
