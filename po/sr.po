msgstr "Примили сте поруку"

#: ../data/phonemgr.ui.h:3
msgid "Sender:"
msgstr "Пошиљалац:"

#: ../data/phonemgr.ui.h:4
msgid "Date:"
msgstr "Датум:"

#: ../data/phonemgr.ui.h:5
msgid "_Reply"
msgstr "_Одговори"

#: ../data/phonemgr.ui.h:6
msgid "Send Message"
msgstr "Пошаљи поруку"

